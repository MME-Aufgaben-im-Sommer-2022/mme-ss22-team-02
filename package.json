{
  "name": "mme-projects",
  "version": "0.0.1",
  "description": "A template for your MME Projects",
  "main": "index.js",
  "watch": {
    "runDevelopementServer": {
      "patterns": [
        "app/src"
      ],
      "extensions": "js",
      "inherit": true,
      "clearBuffer": true
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-watch",
    "runDevelopementServer": "webpack-dev-server --mode development",
    "buildNoDeploy": "webpack --mode production",
    "build": "npm run buildNoDeploy && node build_for_deployment.js",
    "check": "eslint src/",
    "prebuild": "npm install && npm run check"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "babel-cli": "^6.23.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "npm-watch": "^0.11.0",
    "open": "^8.4.0",
    "react-refresh": "^0.14.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "appwrite": "^9.0.1",
    "babel-preset-react-app": "^3.1.2",
    "eslint-plugin-react": "^7.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
